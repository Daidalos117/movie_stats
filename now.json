{
  "version": 2,
  "builds": [
    { "src": "be/app.js", "use": "@now/node-server" },
    {
      "src": "fe/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "be/app.js" },
    { "src": "/about", "dest": "https://about.me" },
    { "src": "/(.*)", "dest": "fe/build/index.html" }
  ]
}
